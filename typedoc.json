{
  "name": "Pipcook APIs",
  "mode": "file",
  "out": "docs/typedoc",
  "theme": "default",
  "inputFiles": ["packages/core/src"],
  "ignoreCompilerErrors": "false",
  "exclude": [
    "packages/cli/**/*.ts",
    "packages/costa/**/*.ts",
    "packages/daemon/**/*.ts",
    "packages/pipboard/**/*.ts",
    "packages/plugins/**/*.ts",
    "**/*+(_test).ts"
  ],
  "preserveConstEnums": "true",
  "stripInternal": "false",
  "excludeExternals": "true",
  "excludeNotExported": "true",
  "readme": "none"
}
