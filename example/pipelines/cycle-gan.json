{
  "plugins": {
    "dataCollect": {
      "package": "@pipcook/plugins-image-classification-data-collec",
      "params": {
        "url": "https://ai-sample.oss-cn-hangzhou.aliyuncs.com/horse2zebra.zip"
      }
    },
    "dataAccess": {
      "package": "@pipcook/plugins-pascalvoc-data-access"
    },
    "modelDefine": {
      "package": "@pipcook/plugins-cycle-gan-model-define",
      "params": {
        "batch_size": 10,
        "save_iter": 200,
        "niter": 100000,
        "lmbd": 10,
        "lr": 0.0002
      }
    },
    "modelTrain": {
      "package": "@pipcook/plugins-cycle-gan-tensorflow-model-train"
    }
  }
}
