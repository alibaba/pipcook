{
  "name": "@pipcook/sdk",
  "version": "1.1.2",
  "description": "JavaScript SDK for accessing Pipcook Daemon",
  "main": "dist/index.js",
  "files": [
    "dist",
    "package-lock.json"
  ],
  "scripts": {
    "build": "npm run clean && npm run compile",
    "dev-build": "npm run clean && npm run dev-compile",
    "clean": "rm -rf ./dist && rm -rf tsconfig.tsbuildinfo",
    "dev-compile": "esbuild ./src/*.ts --tsconfig=tsconfig.json --outdir=dist",
    "compile": "tsc -b tsconfig.json"
  },
  "dependencies": {
    "@pipcook/pipcook-core": "^1.1.2",
    "axios": "^0.18.1",
    "eventsource": "^1.0.7",
    "form-data": "^3.0.0",
    "fs-extra": "^8.1.0"
  },
  "devDependencies": {
    "@types/eventsource": "^1.1.2",
    "@types/form-data": "^2.5.0",
    "@types/fs-extra": "^9.0.1",
    "typescript": "^3.7.2"
  }
}
