{
  "name": "@pipcook/plugins-image-data-process",
  "version": "1.0.3",
  "description": "This plugin is used to process image data. This means we will do some modifications on the original image data you provide from data-collect.",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./dist && rm -rf tsconfig.tsbuildinfo",
    "compile": "tsc -b tsconfig.json"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@pipcook/pipcook-core": "^1.0.3",
    "@types/cli-progress": "^3.4.2",
    "cli-progress": "^3.6.0",
    "jimp": "^0.10.0"
  },
  "gitHead": "53e131a069b3112a74ae5d99aa1ab08bec768c7e",
  "devDependencies": {
    "@types/jasmine": "^3.5.7",
    "nyc": "14.1.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "pipcook": {
    "category": "dataProcess",
    "datatype": "image",
    "params": [{
      "name": "resize",
      "type": "number[2]",
      "description": "the scale to resize"
    }]
  }
}
