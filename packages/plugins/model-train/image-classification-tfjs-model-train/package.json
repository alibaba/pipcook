{
  "name": "@pipcook/plugins-image-classification-tfjs-model-train",
  "version": "1.0.0",
  "description": "This plugin will be used to train image classification models built on top of Tensorflow.js.",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./dist && rm -rf tsconfig.tsbuildinfo",
    "compile": "tsc -b tsconfig.json"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@pipcook/pipcook-core": "^1.0.0",
    "@tensorflow/tfjs-node-gpu": "1.7.0",
    "jimp": "^0.10.0"
  },
  "gitHead": "53e131a069b3112a74ae5d99aa1ab08bec768c7e",
  "devDependencies": {
    "@types/jasmine": "^3.5.7",
    "nyc": "14.1.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "pipcook": {
    "category": "modelTrain",
    "datatype": "image",
    "params": [{
      "name": "epochs",
      "type": "number",
      "description": "how many epochs to train."
    }, {
      "name": "batchSize",
      "type": "number",
      "description": "dataset batch size to be trained for every iteration."
    }]
  }
}
