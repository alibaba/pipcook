{
  "name": "@pipcook/cli",
  "version": "2.1.2",
  "description": "",
  "main": "dist/bin/pipcook.js",
  "scripts": {
    "test": "ava",
    "build": "npm run clean && npm run compile",
    "clean": "((rm -rf ./dist && rm -rf tsconfig.tsbuildinfo) || (rmdir /Q /S ./dist && rmdir /Q /S tsconfig.tsbuildinfo))",
    "compile": "tsc -b tsconfig.json && (chmod +x ./dist/bin/pipcook.js || echo 'windows not support chmod for bin')",
    "cov": "nyc -r text-summary npm run test",
    "cov:report": "nyc report -r lcov"
  },
  "bin": {
    "pipcook": "dist/bin/pipcook.js"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@pipcook/core": "^2.1.1",
    "@pipcook/costa": "^2.1.1",
    "bent": "^7.3.12",
    "chalk": "^3.0.0",
    "cli-progress": "^3.9.0",
    "commander": "^4.0.1",
    "dateformat": "^4.5.1",
    "debug": "^4.3.1",
    "express": "^4.17.1",
    "extract-zip": "^2.0.1",
    "fs-extra": "^8.1.0",
    "jimp": "^0.16.1",
    "multer": "^1.4.3",
    "nanoid": "^3.1.22",
    "ora": "^5.4.1",
    "pretty-bytes": "^5.6.0",
    "query-string": "^6.14.1",
    "semver": "^6.3.0"
  },
  "devDependencies": {
    "@types/bent": "^7.3.2",
    "@types/cli-progress": "^3.9.1",
    "@types/dateformat": "^3.0.1",
    "@types/express": "^4.17.13",
    "@types/extract-zip": "^1.6.2",
    "@types/fs-extra": "^9.0.9",
    "@types/multer": "^1.4.7",
    "@types/node": "^14.6.0",
    "@types/semver": "^7.3.4",
    "@types/sinon": "^9.0.11",
    "ava": "^3.13.0",
    "import-fresh": "^3.3.0",
    "nyc": "^15.1.0",
    "sinon": "^10.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.3.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
