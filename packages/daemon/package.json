{
  "name": "@pipcook/daemon",
  "version": "2.0.0",
  "description": "pipcook daemon",
  "keywords": [
    "pipcook",
    "machine-learning",
    "loopback-application",
    "loopback"
  ],
  "main": "dist",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=12.19"
  },
  "scripts": {
    "compile": "tsc",
    "build": "yarn run compile",
    "build:watch": "tsc --watch",
    "lint": "yarn run eslint",
    "lint:fix": "yarn run eslint:fix",
    "eslint": "eslint .",
    "eslint:fix": "yarn run eslint --fix",
    "pretest": "yarn run rebuild",
    "test": "ava",
    "cov": "nyc --reporter=lcov ava",
    "cov:report": "nyc report --reporter=html",
    "posttest": "yarn run lint",
    "test:dev": "yarn run test && yarn run posttest",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "rebuild": "yarn run clean && yarn run build",
    "prestart": "yarn run rebuild",
    "start": "node -r source-map-support/register .",
    "clean": "rimraf dist *.tsbuildinfo .eslintcache"
  },
  "repository": {
    "type": "git"
  },
  "author": "anonymous",
  "license": "Apache-2.0",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^3.1.0",
    "@loopback/core": "^2.12.0",
    "@loopback/repository": "^3.2.0",
    "@loopback/rest": "^9.0.0",
    "@loopback/rest-explorer": "^3.0.3",
    "@loopback/service-proxy": "^3.0.3",
    "@pipcook/costa": "^1.2.0",
    "@pipcook/pipcook-core": "^1.2.0",
    "axios": "^0.18.1",
    "debug": "^4.3.1",
    "loopback-connector-sqlite3": "git+https://github.com/FeelyChau/loopback-connector-sqlite3.git#36d997d",
    "multer": "^1.4.2",
    "queue": "^6.0.1",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "ssestream": "^1.1.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@types/multer": "^1.4.5",
    "@types/request-promise": "^4.1.46",
    "@loopback/testlab": "^3.2.10",
    "@types/node": "^10.17.48",
    "@loopback/eslint-config": "^10.0.4",
    "ava": "^3.13.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.19",
    "eslint": "^7.15.0",
    "typescript": "^4.1.2"
  }
  
}
