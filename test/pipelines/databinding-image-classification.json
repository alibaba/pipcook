{
  "plugins": {
    "dataCollect": {
      "package": "./packages/plugins/data-collect/image-classification-data-collect",
      "params": {
        "url": "http://ai-sample.oss-cn-hangzhou.aliyuncs.com/image_classification/datasets/eCommerceImageClassification.zip"
      }
    },
    "dataAccess": {
      "package": "./packages/plugins/data-access/pascalvoc-data-access"
    },
    "dataProcess": {
      "package": "./packages/plugins/data-process/image-data-process",
      "params": {
        "resize": [256, 256]
      }
    },
    "modelDefine": {
      "package": "./packages/plugins/model-define/tfjs-mobilenet-model-define"
    },
    "modelTrain": {
      "package": "./packages/plugins/model-train/image-classification-tfjs-model-train",
      "params": {
        "epochs": 15
      }
    },
    "modelEvaluate": {
      "package": "./packages/plugins/model-evaluate/image-classification-tfjs-model-evaluate"
    }
  }
}
